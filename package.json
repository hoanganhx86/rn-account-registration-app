{
  "name": "authentication",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force && yarn",
    "pod:install": "cd ios && pod install --verbose && cd ..",
    "pod:install:clean": "cd ios && rm -rf Podfile.lock && pod install --repo-update --verbose && cd ..",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "xcode": "open ios/authentication.xcworkspace",
    "studio": "open -a /Applications/Android\\ Studio.app ./android/",
    "test": "jest",
    "test:clearCache": "jest --clearCache",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --quiet --fix",
    "quality:check": "yarn lint . && yarn test"
  },
  "dependencies": {
    "@callstack/react-theme-provider": "^3.0.6",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "apisauce": "^2.1.1",
    "color": "^4.0.0",
    "formik": "^2.2.9",
    "qs": "^6.10.1",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-device-info": "^8.1.3",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-iphone-x-helper": "1.3.1",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^7.2.0",
    "@types/color": "^3.0.2",
    "@types/jest": "^26.0.23",
    "@types/qs": "^6.9.7",
    "@types/react-native": "^0.64.5",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "axios-mock-adapter": "^1.19.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
